@using ShivFactory.Business.Repository;
@using ShivFactory.Business.Models.Other;
@{
    Layout = null;
    RepoCookie repoCookie = new RepoCookie();
    var UserName = repoCookie.GetStringCookiesValue(CookieName.FirstName) + " " + repoCookie.GetStringCookiesValue(CookieName.LastName);
}
<script src="https://code.jquery.com/jquery-1.9.1.min.js"></script>

<style>
    .loader-spinner {
        position: fixed;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        z-index: 1000;
        background: url('//i.stack.imgur.com/FhHRx.gif') no-repeat center center;
    }
</style>
<script src="https://code.jquery.com/jquery-1.8.2.js"></script>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
</head>
<body>
  
    <div id="loader" class="center"></div>
    <div class="container-scroller">
        <div class="horizontal-menu">
            <nav class="navbar top-navbar col-lg-12 col-12 p-0">
                <div class="container">
                    <div class="text-center navbar-brand-wrapper d-flex align-items-center justify-content-center">
                        <a class="navbar-brand brand-logo" href="#"><img src="~/Content/AdminImg/shiv  factory logo.png" style="height: 50px" /></a>
                    </div>
                    <div class="navbar-menu-wrapper d-flex align-items-center justify-content-end">
                        <ul class="navbar-nav mr-lg-2">
                            <li class="nav-item nav-search d-none d-lg-block">

                            </li>
                        </ul><ul class="navbar-nav navbar-nav-right">
                            <li class="nav-item nav-profile dropdown">
                                <a class="nav-link dropdown-toggle" id="profileDropdown" href="#" data-toggle="dropdown" aria-expanded="false">
                                    <div class="nav-profile-img">
                                        @*<img src="~/Content/AdminImg/faces-face1.jpg" />*@
                                        <img src="~/Content/AdminImg/faces-face1.jpg" alt="image"><span class="availability-status online"></span>
                                    </div>
                                    <div class="nav-profile-text">
                                        <p class="text-black">@UserName</p>
                                    </div>
                                </a>
                                <div class="dropdown-menu navbar-dropdown" aria-labelledby="profileDropdown">
                                    <a class="dropdown-item" href="#">
                                        <i class="mdi mdi-cached mr-2 text-success"></i> Activity Log
                                    </a>
                                    <div class="dropdown-divider"></div>
                                    <a class="dropdown-item" href="~/Account/LogOff">
                                        <i class="mdi mdi-logout mr-2 text-primary"></i> Signout
                                    </a>
                                </div>
                            </li>
                            @*<li class="nav-item d-none d-lg-flex full-screen-link">
                                    <a class="nav-link" href="demo_3.html">
                                        <i class="mdi mdi-fullscreen" id="fullscreen-button"></i>
                                    </a>
                                </li>*@
                            <li class="nav-item dropdown">
                                <a class="nav-link count-indicator dropdown-toggle" id="messageDropdown" href="#" data-toggle="dropdown" aria-expanded="false">
                                    <i class="mdi mdi-email-outline"></i>
                                    <span class="count-symbol bg-warning"></span>
                                </a>
                                <div class="dropdown-menu dropdown-menu-right navbar-dropdown preview-list" aria-labelledby="messageDropdown">
                                    <h6 class="p-3 mb-0">Messages</h6>
                                    <div class="dropdown-divider"></div>
                                    <a class="dropdown-item preview-item" href="demo_3.html">
                                        @*<div class="preview-thumbnail">
                                                <img src="images/faces-face4.jpg" alt="image" class="profile-pic">
                                            </div>*@
                                        <div class="preview-item-content d-flex align-items-start flex-column justify-content-center">
                                            <h6 class="preview-subject ellipsis mb-1 font-weight-normal">Mark send you a message</h6>
                                            <p class="text-gray mb-0"> 1 Minutes ago </p>
                                        </div>
                                    </a>
                                    <div class="dropdown-divider"></div>
                                    <h6 class="p-3 mb-0 text-center">4 new messages</h6>
                                </div>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link count-indicator dropdown-toggle" id="notificationDropdown" href="#" data-toggle="dropdown">
                                    <i class="mdi mdi-bell-outline"></i>
                                    <span class="count-symbol bg-danger"></span>
                                </a>
                                <div class="dropdown-menu dropdown-menu-right navbar-dropdown preview-list" aria-labelledby="notificationDropdown">
                                    <h6 class="p-3 mb-0">Notifications</h6>
                                    <div class="dropdown-divider"></div>
                                    <a class="dropdown-item preview-item" href="demo_3.html">
                                        <div class="preview-thumbnail">
                                            <div class="preview-icon bg-success">
                                                <i class="mdi mdi-calendar"></i>
                                            </div>
                                        </div>
                                        <div class="preview-item-content d-flex align-items-start flex-column justify-content-center">
                                            <h6 class="preview-subject font-weight-normal mb-1">Event today</h6>
                                            <p class="text-gray ellipsis mb-0"> Just a reminder that you have an event today </p>
                                        </div>
                                    </a>
                                    <div class="dropdown-divider"></div>
                                    <a class="dropdown-item preview-item" href="demo_3.html">
                                        <div class="preview-thumbnail">
                                            <div class="preview-icon bg-warning">
                                                <i class="mdi mdi-settings"></i>
                                            </div>
                                        </div>
                                        <div class="preview-item-content d-flex align-items-start flex-column justify-content-center">
                                            <h6 class="preview-subject font-weight-normal mb-1">Settings</h6>
                                            <p class="text-gray ellipsis mb-0"> Update dashboard </p>
                                        </div>
                                    </a>
                                    <div class="dropdown-divider"></div>
                                    <a class="dropdown-item preview-item" href="demo_3.html">
                                        <div class="preview-thumbnail">
                                            <div class="preview-icon bg-info">
                                                <i class="mdi mdi-link-variant"></i>
                                            </div>
                                        </div>
                                        <div class="preview-item-content d-flex align-items-start flex-column justify-content-center">
                                            <h6 class="preview-subject font-weight-normal mb-1">Launch Admin</h6>
                                            <p class="text-gray ellipsis mb-0"> New admin wow! </p>
                                        </div>
                                    </a>
                                    <div class="dropdown-divider"></div>
                                    <h6 class="p-3 mb-0 text-center">See all notifications</h6>
                                </div>
                            </li>
                        </ul>
                        <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="horizontal-menu-toggle">
                            <span class="mdi mdi-menu"></span>
                        </button>
                    </div>
                </div>
            </nav>
            @*Render navvar Partial view*@
            @{ Html.RenderPartial(Url.Content("../Home/NavPartialView")); }
        </div>
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
        @RenderBody()
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/toastr")
        @RenderSection("scripts", required: false)
    </div>
    <footer class="footer">
        <div class="container">
            <div class="d-sm-flex justify-content-center justify-content-sm-between">
                <span class="text-muted text-center text-sm-left d-block d-sm-inline-block">Copyright &copy; 2017 <a href="www.bootstrapdash.html" target="_blank">Shivfactory</a>. All rights reserved.</span>
                <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">Hand-crafted &amp; made with <i class="mdi mdi-heart text-danger"></i></span>
            </div>
        </div>
    </footer>

    <link href="~/Content/toastr.css" rel="stylesheet" />
    <script src="~/Content/JS/js-vendor.bundle.base.js"></script>
    <script src="~/Content/JS/datatables.net-jquery.dataTables.js"></script>
    <script src="~/Content/JS/datatables.net-bs4-dataTables.bootstrap4.js"></script>
    <link href="~/Content/CSS/datatables.net-bs4-dataTables.bootstrap4.css" rel="stylesheet" />


    <link rel="stylesheet" href="~/Content/CSS/demo_3-style.css">
    <link href="~/Content/CSS/css-materialdesignicons.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/CSS/css-font-awesome.min.css">
    @*<link href="~/Content/CSS/buttons.dataTables.min.css" rel="stylesheet" />
        <link href="~/Content/CSS/jquery.dataTables.min.css" rel="stylesheet" />*@
    @*<script src="~/Content/JS/dataTableAndTools.js"></script>*@

    <script src="~/Content/JS/chart.js-Chart.min.js"></script>
    <script src="~/Content/JS/js-settings.js"></script>
    <script src="~/Content/JS/js-dashboard.js"></script>

    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Content/JS/Global.js"></script>
    <script src="~/Content/JS/CommonFunction.js"></script>
    <script src="~/Content/JS/Common.js"></script>
    <script src="~/Content/JS/Product.js"></script>
    @*Showing toaster message*@
    <script src="~/Scripts/toastr.js"></script>
    @if (TempData["SuccessMessage"] != null)
    {
        <script>
    toastr.success('@TempData["SuccessMessage"].ToString()');
        </script>
    }
    else if (TempData["ErrorMessage"] != null)
    {
        <script>
            toastr.error('@TempData["ErrorMessage"].ToString()');
        </script>
    }
</body>
</html>

