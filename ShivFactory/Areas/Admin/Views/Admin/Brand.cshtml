
@{
    ViewBag.Title = "Brand";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutDashboard.cshtml";
}

<div class="col-lg-12 stretch-card">
    <div class="card">
        <div class="card-body">
            @using (Html.BeginForm("AddBrand", "Admin", FormMethod.Get))
            {
                <p class="card-description" style=" text-align: end;">
                    <button type="submit" class="btn btn-primary">
                        Add Brand
                    </button>
                </p>
            }
            <div class="partialView">
            </div>
        </div>
    </div>
</div>


<script type="text/javascript">
    $(document).ready(function () {
        common.RenderPartialView('Admin', 'BrandPartialView');
    });
</script>

