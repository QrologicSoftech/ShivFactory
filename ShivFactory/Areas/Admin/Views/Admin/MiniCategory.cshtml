
@{
    ViewBag.Title = "MiniCategory";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutDashboard.cshtml";
}
@*<div id="snackbar"></div>*@
<div class="col-lg-12 stretch-card">
    <div class="card">
        <div class="card-body">
            @using (Html.BeginForm("AddMiniCategory", "Admin", FormMethod.Get))
            {
                <p class="card-description" style=" text-align: end;">
                    <button type="submit" class="btn btn-primary">
                        Add Category
                    </button>
                </p>
            }
            <div class="partialView">

            </div>
            @*@{ Html.RenderPartial("CategoryPartialView"); }*@
        </div>
    </div>
</div>
