    <script type="text/javascript">
        $(function () {

            $("#txtsearch-item").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: '/Home/ProductAutoComplete/',
                        data: "{ 'prefix': '" + request.term + "'}",
                        dataType: "json",
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        success: function (res) {
                            if (res.ResultFlag) {
                                response($.map(res.Data, function (item) {
                                    //return { label: item.Value, value: item.Id };
                                    return { Text: item.Id, label: item.Value, value: item.Value };
                                }))
                            }
                            else {
                                var result = [
                                    {
                                        label: 'No matches found',
                                        value: null
                                    }
                                ];
                                response(result);
                            }
                        },
                        error: function (response) {
                            toastr.error(response.Message);
                        },
                        failure: function (response) {
                            toastr.error(response.Message);
                        }
                    });
                },
                select: function (e, i) {
                    if (i.item.value == null) {
                        $("#txtsearch-item").val("");
                        return;
                    }
                    location.replace(`/Home/ProductListing?subId=${i.item.Text}&searchtext=${i.item.value}`);
                    const urlParams = new URLSearchParams(window.location.search);
                    const myParam = urlParams.get('searchtext');
                    $("#txtsearch-item").val(myParam);
                    console.log(myParam);
                },
                minLength: 1
            });
        });

