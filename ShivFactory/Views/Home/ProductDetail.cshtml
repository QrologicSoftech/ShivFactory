@model ShivFactory.Business.Repository.ProductDetail
@{
    ViewBag.Title = "ProductDetail";
    Layout = "~/Views/Shared/_WebSiteLayout.cshtml";
}
<script src="~/Content/Website/js/imagezoom.js"></script>
<script src="~/Content/Website/js/jquery.flexslider.js"></script>
<script src="~/Js/Cart.js"></script>
@using (Html.BeginForm("AddToCart", "Home", FormMethod.Post, new { id = "myform", @class = "forms-sample" }))
{
    if (Model != null)
    {
        @*<section class="py-3 bg-light my-5">
                <div class="container">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item"><a href="#">Category name</a></li>
                        <li class="breadcrumb-item"><a href="#">Sub category</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Items</li>
                    </ol>
                </div>
            </section>*@
        @* var item=Model.varientList;*@
        var MainimgPath = Model.MainImage;
        var imgPath1 = Model.Image1;
        var imgPath2 = Model.Image2;
        var imgPath3 = Model.Image3;
        var imgPath4 = Model.Image4;
        var imgPath5 = Model.Image5;
        var discount_percentage = "0.00";
        try
        {
            var discount = @Model.ListPrice - Model.SalePrice;
            discount_percentage = Convert.ToDecimal(discount / @Model.ListPrice * 100).ToString("0.00") + "%Off"; ;
        }
        catch (Exception e)
        { }

        <section>
            <input type="hidden" id="ProductId" value="@Model.ProductId" />
            <input type="hidden" id="ProductVarientId" value="@Model.ProductVarientId" />
            <input type="hidden" id="vendorId" value="@Model.vendorId" />
            <input type="hidden" id="Quantity" value="1" />
            <div class="container">
                <div class="row" style="background-color: rgb(255, 255, 255); padding: 16px;">
                    <div class="col-md-5">
                        <div class="row">
                            <div class="img-prodecu-de">
                                <div class="men-div">
                                    <div class="sun-1">
                                        <div class="menu-2">
                                            <div class="menu-0" style="height: 342.857px;">
                                                <ul class="menu-1" style="transform: translateY(0px);">
                                                    <li class="menu-sun demo" style="height: 68.5714px;">
                                                        <div class="sun color-text-1 color-hover">
                                                            <div class="sunimg"><img src="@MainimgPath"></div>
                                                        </div>
                                                    </li>
                                                    <li class="menu-sun demo-1" style="height: 68.5714px;">
                                                        <div class="sun color-text-2">
                                                            <div class="sunimg"><img src="@imgPath1"></div>
                                                        </div>
                                                    </li>
                                                    <li class="menu-sun demo-2" style="height: 68.5714px;">
                                                        <div class="sun color-text-3">
                                                            <div class="sunimg"><img src="@imgPath2"></div>
                                                        </div>
                                                    </li>
                                                    <li class="menu-sun demo-3" style="height: 68.5714px;">
                                                        <div class="sun color-text-4">
                                                            <div class="sunimg"><img src="@imgPath3"></div>
                                                        </div>
                                                    </li>
                                                    <li class="menu-sun demo-4" style="height: 68.5714px;">
                                                        <div class="sun color-text-5">
                                                            <div class="sunimg"><img src="@imgPath4"></div>
                                                        </div>
                                                    </li>
                                                </ul>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="img-prodecu-de-1">
                                <div class="full-img-2">
                                    <div class="images-1">
                                        <div data-thumb="@MainimgPath">
                                            <img src="@MainimgPath" data-imagezoom="true" class="img-responsive images-1-0 hover-1">
                                        </div>
                                        <div data-thumb="@imgPath1">
                                            <img src="@imgPath1" data-imagezoom="true" class="images-1-1">
                                        </div>
                                        <div data-thumb="@imgPath2">
                                            <img src="@imgPath2" data-imagezoom="true" class="images-1-2">
                                        </div>
                                        <div data-thumb="@imgPath3">
                                            <img src="@imgPath3" data-imagezoom="true" class="images-1-3">
                                        </div>
                                        <div data-thumb="@imgPath4">
                                            <img src="@imgPath4" data-imagezoom="true" class="images-1-4">
                                        </div>

                                        @if (Model.varientList != null)
                                        {
                                            var k = 5;
                                            foreach (var img in Model.varientList)
                                            {
                                                var classname = "images-1-" + k;
                                                <div data-thumb="@img.ImagePath">
                                                    <img src="@img.ImagePath" data-imagezoom="true" class="@classname">
                                                </div>
                                                k++;
                                            }
                                        }
                                        @*<div data-thumb="img/product-detail-img/product/2-1.jpeg">
                                                <img src="img/product-detail-img/product/2-1.jpeg" data-imagezoom="true" class="images-1-9">
                                            </div>

                                            <div data-thumb="img/product-detail-img/product/2-1.jpeg">
                                                <img src="img/product-detail-img/product/2-1.jpeg" data-imagezoom="true" class="images-1-10">
                                            </div>*@

                                    </div>
                                    <div class="like-icon">
                                        <a href="#" onclick="cart.AddToCart(true)"><i class="fa fa-heart" aria-hidden="true"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="btn-right">
                                    <button type="button" onclick="cart.AddToCart(false)" class="btn btn-warning btn-btom"><i class="fa fa-shopping-cart" aria-hidden="true"></i> ADD TO CART</button>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="btn-left">
                                    <button type="button" class="btn btn-warning btn-btom-1"> BUY NOW</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-7">
                        <div class="sunglas">
                            <ul>
                                <li><a href="/Home/">Home </a></li>
                                <li><a href="/Home/ProductListing?id=@Model.CategoryId.ToString().Trim()">@Model.CategoryName</a></li>
                                <li><a href="/Home/ProductListing?subId=@Model.SubCategoryId.ToString().Trim()">@Model.SubCategoryName </a></li>
                                <li><a href="/Home/ProductListing?subId=@Model.SubCategoryId.ToString().Trim()&minId=@Model.MiniCategoryId.ToString().Trim()">@Model.MiniCategoryName </a></li>
                                <li><a>@Model.ProductName.Substring(0, Model.ProductName.Length / 2)..</a></li>
                                <li><a href="#"><i class="fa fa-share" aria-hidden="true"></i> Share</a></li>
                            </ul>
                            <h3> <!--Brand Name--> </h3>
                            <p id="ProductName">@Model.ProductName</p>
                            <div class="offer">
                                <div class="offer-1" id="ddd"><span>₹</span><span id="SalePrice"> @Model.SalePrice</span></div>
                                <div class="offer-2">₹@Model.ListPrice</div>
                                <div class="offer-3">@discount_percentage</div>
                                <div class="icon-img"><img src="~/Content/UploadedImages/Images/icon.svg"></div>
                            </div>
                            <div class="star">
                                <span>4.5 <i class="fa fa-star" aria-hidden="true"></i></span>
                                <p>4 ratings and 0 reviews</p>
                                @*<img src="">*@
                            </div>
                            <h2>Available offers</h2>
                        </div>
                        <div class="available-offers">

                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="col-12" style="padding: 32px 0px 0px 0px;">
                                    <div class="deliver">
                                        <img src="~/Content/UploadedImages/Images/map.svg" />
                                        <span class="deliver-1">Deliver to</span>
                                    </div>
                                    <div class="input-text">
                                        <input type="tel" placeholder="Enter delivery pincode" id="pincode" value="" maxlength="6" autocomplete="off" class="cfnctZ">
                                        <span id="chkpincode" onclick="Listing.CheckPincodeAvailibity(); ">Check</span>
                                        <div id="reponsePinCode">
                                            <b>
                                                <label id="lblDeliery">

                                                </label>
                                            </b>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6" style="padding: 32px 0px 0px 52px;">
                                <div class="return-service">Services</div>
                                <div class="service-rut">
                                    <ul>
                                        <li>
                                            <div class="return">
                                                <img src="img/download-1.svg">
                                            </div>
                                            @if (@Model.IsReturnable == true)
                                            {
                                                <span>@Model.ReturnDays Days Return Policy</span>
                                            }
                                            else
                                            {
                                                <span>No Return Policy </span>
                                            }
                                            <div class="Bvquestion">
                                                <span class="question" title="return Policy">?</span>
                                            </div>
                                        </li>
                                        <li>
                                            <div class="return">
                                                <img src="img/download-2.svg">
                                            </div>
                                            @if (Model.paymentModeCash == true)
                                            {
                                                <span>Cash on delivery available</span>
                                            }
                                            else
                                            {
                                                <span>Cash on delivery not available</span>
                                            }
                                            <div class="Bvquestion">
                                                <span class="question" title="return Policy">?</span>
                                            </div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="deliver-day">
                                    <h4 id="deliverytime" style="display:none">Usually delivered in @Model.EstimateDeliveryTime </h4>
                                    <div class="Bvquestion">
                                        <span class="question">?</span>
                                    </div>
                                    <p>Enter pincode for exact delivery dates/charges</p>
                                    <span>View Details</span>
                                </div>

                                @if (Model.varientList != null)
                                {
                                    foreach (var item in Model.varientList)
                                    {
                                        <div class="sizes">
                                            <span>@item.VarientName</span>
                                            <ul>
                                                @foreach (var varientval in item.VarientValue)
                                                {

                                                    var url = "/Home/ProductDetail?productId=" + @Model.ProductId + "&VarientName=" + @item.VarientName + "&VarientValue=" + @varientval;
                                                    <li>
                                                        <a href='@url'>@varientval</a>
                                                    </li>
                                                }
                                            </ul>

                                        </div>
                                    }
                                }


                                <div class="sizes">
                                    <span>Images</span>
                                    <ul class="menu-1" style="transform: translateY(0px);">
                                        @if (Model.varientList != null)
                                        {
                                            int k = 6;
                                            foreach (var img in Model.varientList)
                                            {

                                                var liclassname = "menu-sun demo-" + k;
                                                var divclassname = "sun color-text-" + k;
                                                <li class="@liclassname" style="height: 68.5714px;">
                                                    <div class="@divclassname">
                                                        <div class="sunimg"><img src="@img.ImagePath"></div>
                                                    </div>
                                                </li>
                                                k = k + 1;
                                            }
                                        }
                                    </ul>
                                </div>
                                @*<div class="sizes">
                                        <span>Images</span>
                                        <ul class="menu-1" style="transform: translateY(0px);">
                                            <li class="menu-sun demo" style="height: 68.5714px;">
                                                <div class="sun color-text-1">
                                                    <div class="sunimg"><img src="~/Content/UploadedImages/Images/NoImg.png"></div>
                                                </div>
                                            </li>
                                            <li class="menu-sun demo-6" style="height: 68.5714px;">
                                                <div class="sun color-text-6">
                                                    <div class="sunimg"><img src=""></div>
                                                </div>
                                            </li>
                                            <li class="menu-sun demo-7" style="height: 68.5714px;">
                                                <div class="sun color-text-7">
                                                    <div class="sunimg"><img src="~/Content/UploadedImages/Images/NoImg.png"></div>
                                                </div>
                                            </li>
                                            <li class="menu-sun demo-8" style="height: 68.5714px;">
                                                <div class="sun color-text-8">
                                                    <div class="sunimg"><img src=""></div>
                                                </div>
                                            </li>
                                            <li class="menu-sun demo-9" style="height: 68.5714px;">
                                                <div class="sun color-text-9">
                                                    <div class="sunimg"><img src=""></div>
                                                </div>
                                            </li>
                                        </ul>
                                    </div>*@

                                <div class="deliver-day">
                                    <h4>Description</h4>
                                    @*<div class="Bvquestion">
                                            <span class="question"></span>
                                        </div>*@
                                    <p>@Model.Description</p>
                                    <span>View More</span>
                                </div>
                                <div class="seller" style="padding: 8px 0px 0px 0px;">
                                    <div class="seller-1">
                                        <div class="seller-text">
                                            <span>Seller</span>
                                        </div>
                                        <div class="collection">
                                            @Model.FirmName
                                            <div class="collection-1">
                                                3.3
                                                <i class="fa fa-star" aria-hidden="true"></i>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="return-policy">
                                        <ul>
                                            @if (@Model.IsReturnable == true)
                                            {
                                                <li>
                                                    <div class="policy-1">
                                                        @Model.ReturnDays Days Return Policy
                                                        <div class="Bvquestion">
                                                            <span class="question">?</span>
                                                        </div>
                                                    </div>
                                                </li>
                                            }
                                            <li>
                                                <span><a href="#">View more sellers </a><small>starting from <b>₹149</b></small></span>
                                            </li>

                                        </ul>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

